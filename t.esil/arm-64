#!/bin/sh

for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

### load cases ###

NAME="ldr x25, [sp, 0x28]"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
ar sp=4
wx f91740f9
w0 0x28@4
wx 8877665544332211cacacacacaca@0x2c
aes
ar x25
'
EXPECT='0x1122334455667788
'
run_test

NAME="ldur x25, [sp, 0x28]"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
ar sp=4
wx f91740f9
w0 0x28@4
wx 8877665544332211cacacacacaca@0x2c
aes
ar x25
'
EXPECT='0x1122334455667788
'
run_test

NAME="ldr w12, [sp, 0x1c]"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
ar sp=4
wx ec1f40b9
w0 0x1c@4
wx 8877665544332211cacacacacaca@0x20
aes
ar w12
'
EXPECT='0x55667788
'
run_test

NAME="ldrsw x9, [x8, 0x468]"
FILE=malloc://0x200
ARGS='-a arm -b 64 -m 0x464'
CMDS='
wx 096984b9@0x464
wx 8877665544332211cacacacacaca@0x468
aei
aeim
aeip
aes
ar x9
'
EXPECT='0x55667788
'
run_test

NAME="ldrb w10, [x16]"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
