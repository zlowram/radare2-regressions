### arithmetics ###

NAME="movn w12, 0"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
wx 0c008012
aes
ar w12
'
EXPECT='0xffffffff
'
run_test

NAME="orn w12, w13, w15, lsr 16"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
ar w15=0xaabbccdd
ar w13=0x000000ff
wx ac416f2a
aes
ar w12
'
EXPECT='0xffff55ff
'
run_test

# eor
NAME="eor w11, w12, w15, lsr 16"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
ar w15=0xaabbccdd
ar w12=0xffffffff
wx 8b414f4a
aes
ar w11
'
EXPECT='0xffff5544
'
run_test

NAME="eor w11, w11, w11, asr 31 (positive)"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
ar w11=4
wx 6b7d8b4a
aes
ar w11
'
EXPECT='0x00000004
'
run_test

NAME="eor w11, w11, w11, asr 31 (negative)"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
ar w11=-4
wx 6b7d8b4a
aes
ar w11
'
EXPECT='0x00000003
'
run_test

NAME="eor x20, x21, x21, asr 63 (negative)"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
ar x21=-4
wx b4fe95ca
aes
ar x20
'
EXPECT='0x00000003
'
run_test

NAME="eor w11, w11, 0x80000000"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
ar w11=-4
wx 6b010152
aes
ar w11
'
EXPECT='0x7ffffffc
'
run_test

NAME="eor w11, w11, w13"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
ar w11=-4
ar w13=0x11223344
wx 6b010d4a
aes
ar w11
'
EXPECT='0xeeddccb8
'
run_test

NAME="eor w11, w11, w15, ror 11"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
ar w11=0xffeeddcc
ar w15=0x7ffffffc
wx 6b2dcf4a
aes
ar w11
'
EXPECT='0x00612233
'
run_test

NAME="eon x13, x14, x14, lsl 45"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
ar x14=0xf982834129348123
wx cdb52eca
aes
ar x13
'
EXPECT='0x96591cbed6cb7edc
'
run_test

# zero register

NAME="orr w13, wzr, 0xff"
FILE=malloc://0x200
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
wx ed1f0032
aes
ar w13
'
EXPECT='0x000000ff
'
run_test

NAME="lsl wzr, w0, 0x1f"
FILE=malloc://0x200
BROKEN=1 
CMDS='
e asm.arch=arm
e asm.bits=64
ar > /dev/null
ar w0=0xffffffff
wx 1f000153
aes
ar wzr
'
EXPECT='0x00000000
'
run_test

NAME="eon xzr, x14, x19, asr 39"
FILE=malloc://0x200
