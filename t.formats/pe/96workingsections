#!/bin/sh
for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

NAME='PE: corkami 96workingsections.exe - open and analyze'
FILE=../../bins/pe/96workingsections.exe
ARGS='-A'
CMDS="q!"
EXPECT=''
IGNORE_ERR=1
run_test

NAME='PE: corkami 96workingsections.exe - entrypoint'
FILE=../../bins/pe/96workingsections.exe
CMDS='
s
'
EXPECT='0x461000
'
IGNORE_ERR=1
run_test

