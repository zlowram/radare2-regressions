NAME="section text start (not mapped)"
FILE=../../bins/elf/analysis/main_wrong_sect
ARGS=
CMDS='?v section..text
'
EXPECT='0x410570
'

run_test

NAME="only phdrs mapped"
FILE=../../bins/elf/analysis/main_wrong_sect
ARGS=
CMDS='p8 10 @ section..text
'
EXPECT='ffffffffffffffffffff
'

run_test
