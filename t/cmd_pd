pd 1 @ 0x004016ca
pd 1 @ 0x004016d4
'
EXPECT='            0x004016ac      mov edi, 0x40224a                          ; u"utf16le> \\u00a2\\u20ac\\U00010348 in green:\x1b[32m \xa2\u20ac\U00010348 \x1b[0m\n"
